{
  "name": "react-sizer",
  "version": "1.0.2",
  "description": "Simple React HOC to measure the size of a component",
  "keywords": [
    "React",
    "size",
    "with",
    "height",
    "HOC"
  ],
  "author": "jharris4",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jharris4/react-sizer/issues"
  },
  "homepage": "https://github.com/jharris4/react-sizer#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jharris4/react-sizer.git"
  },
  "main": "lib/index.js",
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-object-rest-spread",
      "transform-react-jsx"
    ]
  },
  "peerDependencies": {
    "react": "^0.14.0 || >=15",
    "react-dom": "^0.14.0 || >=15"
  },
  "dependencies": {
    "hoist-non-react-statics": "^2.3.1",
    "raf": "^3.4.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.1.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "cross-env": "^5.1.3",
    "eslint": "^4.14.0",
    "eslint-plugin-import": "^2.8.0",
    "expect": "1.20.2",
    "karma": "^2.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.9",
    "mocha": "^4.1.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "rimraf": "^2.6.2",
    "webpack": "^3.10.0"
  },
  "scripts": {
    "clean": "rimraf lib",
    "build": "npm run clean && cross-env NODE_ENV=production babel src/ --out-dir lib",
    "lint": "eslint src",
    "test": "npm run lint && karma start"
  }
